{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "declaration": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@chooks22/discord.jsx",
    "lib": [
      "ESNext"
    ]
  },
  "include": [
    "src",
    "jsx.d.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}